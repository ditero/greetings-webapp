<a href="/"><input type="button" class="btn btn-primary btn-sm" name="" value="Greet Again"></a>
<div class="container">
  <div class="col-md-6"></div>
  <div class="center-block">
    <h5 class="text-center">List of greeted names</h5>
    <table class = "table table-striped table-bordered table-hove">
      <thead>
        <tr>
          <!-- <th>#</th> -->
          <th>N#</th>
          <th>Name</th>
        </tr>
      </thead>

      <tbody>

        {{#each myusers}}
        <tr class = "success">
          <td>{{mylist}}</td>
          <td>
            <a href="counter/{{name}}">{{name}}</a>
          </td>
        </tr>
        {{/each}}

      </tbody>

    </table>
  </div>
</div>
